{
  "show_inline_completions": false,
  "features": {
    "inline_completion_provider": "supermaven"
  },
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "tab_size": 2,
  "ui_font_size": 19,
  "buffer_font_size": 19,
  "theme": "Mellow",
  "file_types": {
    "Haskell": ["tidal"]
  }
}
